  "version": "0.2.0",
  "configurations": [
    {
      "name": "SnipShapeDev",
      "type": "autohotkey",
      "request": "launch",
      "program": "${workspaceFolder}/SnipShape.ahk2",
      "cwd": "${workspaceFolder}",
      "args": [],
      // https://github.com/zero-plusplus/vscode-autohotkey-debug/issues/142
